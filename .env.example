# App
NODE_ENV=development
NEXT_PUBLIC_API_URL=http://localhost:3000
PORT=3000

# Security
NEXT_PUBLIC_GITHUB_TOKEN=your_github_token
NEXT_PUBLIC_NFT_CONTRACT_ADDRESS=your_contract_address
NETWORK_RPC_URL=your_rpc_url
WALLET_ADDRESS=your_wallet_address

# Storage
ZERO_G_STORAGE_API=your_0g_api_url
ZERO_G_STORAGE_KEY=your_0g_key

# Monitoring
SENTRY_DSN=your_sentry_dsn
NEXT_PUBLIC_SENTRY_DSN=your_public_sentry_dsn

# Analytics
NEXT_PUBLIC_GA_ID=your_ga_id

# Performance
ANALYZE=false

# Cache
REDIS_URL=your_redis_url

# API Rate Limiting
RATE_LIMIT_MAX=100
RATE_LIMIT_WINDOW_MS=60000

# ThirdWeb SDK API 키 (https://thirdweb.com/dashboard/settings/api-keys)
THIRDWEB_API_KEY=your_thirdweb_api_key
THIRDWEB_SECRET_KEY=your_secret_key

# NFT 컨트랙트 주소 (ThirdWeb 대시보드에서 생성)
NFT_CONTRACT_ADDRESS=your_contract_address

# ELIZA OS Agent API 키 (https://elizaos.com/dashboard)
ELIZA_API_KEY=your_eliza_api_key

# 0G Storage API 키 (https://0g.io/dashboard)
ZEROG_API_KEY=your_0g_api_key

# GitHub Personal Access Token (https://github.com/settings/tokens)
GITHUB_TOKEN=your_github_token

# Database
POSTGRES_URL=your_postgres_url

# ThirdWeb Client
NEXT_PUBLIC_THIRDWEB_CLIENT_ID=your_thirdweb_client_id